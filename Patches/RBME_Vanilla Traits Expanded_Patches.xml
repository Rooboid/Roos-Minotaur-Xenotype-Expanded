	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Traits Expanded</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/BackstoryDef[defName="RBME_BathServant"]</xpath>
